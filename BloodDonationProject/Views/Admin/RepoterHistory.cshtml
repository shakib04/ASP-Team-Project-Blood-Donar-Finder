@model IEnumerable<BloodDonationProject.Models.report>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RepoterHistory</title>
</head>
<body>


    <table class="table">
        <tr>
            <th>
                Report
            </th>
            <th>
                @Html.DisplayNameFor(model => model.userInfo.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.userInfo1.Name)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Report1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.userInfo.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.userInfo1.Name)
                </td>
                <td>
                    @(TempData["idd"] = item.ReportId)
                </td>
            </tr>
        }

    </table>
    @Html.ActionLink("Ban", "BanUser", new { id = @TempData["idd"] })
</body>
</html>
